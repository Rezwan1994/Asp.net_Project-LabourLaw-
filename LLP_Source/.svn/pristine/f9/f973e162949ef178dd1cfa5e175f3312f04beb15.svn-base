CREATE PROCEDURE UpdateEmployee
(
	@Id int,
	@EmployeeId uniqueidentifier,
	@UserId uniqueidentifier,
	@Title nvarchar(50),
	@FirstName nvarchar(250),
	@LastName nvarchar(250),
	@Email nvarchar(50),
	@Street nvarchar(500),
	@City nvarchar(50),
	@State nvarchar(50),
	@ZipCode nvarchar(50),
	@Country nvarchar(50),
	@Phone nvarchar(50),
	@SSN nvarchar(50),
	@IsActive bit,
	@IsDeleted bit,
	@IsInstaller bit,
	@IsSalesPerson bit,
	@IsServiceCall bit,
	@HireDate datetime,
	@ProfilePicture nvarchar(250),
	@Session nvarchar(50),
	@JobTitle nvarchar(100),
	@PlaceOfBirth nvarchar(100),
	@SalesCommissionStructure nvarchar(100),
	@TechCommissionStructure nvarchar(100),
	@RecruitmentProcess bit,
	@Recruited bit,
	@CreatedDate datetime,
	@Status nvarchar(50),
	@LastUpdatedBy nvarchar(250),
	@LastUpdatedDate datetime
)
AS
    UPDATE [dbo].[Employee] 
	SET
	[EmployeeId] = @EmployeeId,
	[UserId] = @UserId,
	[Title] = @Title,
	[FirstName] = @FirstName,
	[LastName] = @LastName,
	[Email] = @Email,
	[Street] = @Street,
	[City] = @City,
	[State] = @State,
	[ZipCode] = @ZipCode,
	[Country] = @Country,
	[Phone] = @Phone,
	[SSN] = @SSN,
	[IsActive] = @IsActive,
	[IsDeleted] = @IsDeleted,
	[IsInstaller] = @IsInstaller,
	[IsSalesPerson] = @IsSalesPerson,
	[IsServiceCall] = @IsServiceCall,
	[HireDate] = @HireDate,
	[ProfilePicture] = @ProfilePicture,
	[Session] = @Session,
	[JobTitle] = @JobTitle,
	[PlaceOfBirth] = @PlaceOfBirth,
	[SalesCommissionStructure] = @SalesCommissionStructure,
	[TechCommissionStructure] = @TechCommissionStructure,
	[RecruitmentProcess] = @RecruitmentProcess,
	[Recruited] = @Recruited,
	[CreatedDate] = @CreatedDate,
	[Status] = @Status,
	[LastUpdatedBy] = @LastUpdatedBy,
	[LastUpdatedDate] = @LastUpdatedDate
	WHERE ( Id = @Id )

	DECLARE @Err int
	DECLARE @Result int
	SET @Result = @@ROWCOUNT
	SET @Err = @@ERROR 

	If @Err <> 0 
	BEGIN
		SET @Result = -1
	END

	RETURN @Result
GO

