/****** Object:  StoredProcedure [dbo].GetPagedEmployee    Script Date: 3/28/2018 2:04:40 PM  ******/
IF  EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'[dbo].[GetPagedEmployee]') AND type in (N'P', N'PC'))
DROP PROCEDURE [dbo].[GetPagedEmployee]
GO
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE PROCEDURE GetPagedEmployee
(
	@TotalRows		int	OUTPUT,
	@PageIndex	int,
	@RowPerPage		int,
	@WhereClause	nvarchar(4000),
	@SortColumn		nvarchar(128),
	@SortOrder		nvarchar(4)
)
AS
BEGIN 

SET @PageIndex = isnull(@PageIndex, -1)
SET @RowPerPage = isnull(@RowPerPage, -1)
SET @WhereClause = isnull(@WhereClause, '')
SET @SortColumn = isnull(@SortColumn, '')
SET @SortOrder = isnull(@SortOrder, '')
SET @TotalRows = 0
SET @RowPerPage = @RowPerPage -1
DECLARE @SQL1 nvarchar(4000)
DECLARE @SQL2 nvarchar(4000)

IF (@WhereClause != '')
BEGIN
	SET @WhereClause = 'WHERE ' + char(13) + @WhereClause	
END

IF (@SortColumn != '')
BEGIN
	SET @SortColumn = 'ORDER BY ' + @SortColumn

	IF (@SortOrder != '')
	BEGIN
		SET @SortColumn = @SortColumn + ' ' + @SortOrder
	END
END
ELSE
BEGIN
	SET @SortColumn = @SortColumn + ' ORDER BY [Id] ASC'
END

SET @SQL1 = 'WITH EmployeeEntries AS (
			SELECT ROW_NUMBER() OVER ('+ @SortColumn +')AS Row,
	[Id],
	[EmployeeId],
	[UserId],
	[Title],
	[FirstName],
	[LastName],
	[Email],
	[Street],
	[City],
	[State],
	[ZipCode],
	[Country],
	[Phone],
	[SSN],
	[IsActive],
	[IsDeleted],
	[IsInstaller],
	[IsSalesPerson],
	[IsServiceCall],
	[HireDate],
	[ProfilePicture],
	[Session],
	[JobTitle],
	[PlaceOfBirth],
	[SalesCommissionStructure],
	[TechCommissionStructure],
	[RecruitmentProcess],
	[Recruited],
	[CreatedDate],
	[Status],
	[LastUpdatedBy],
	[LastUpdatedDate]
				FROM 
				[dbo].[Employee]
					'+ @WhereClause +'
				)
				SELECT 
	[Id],
	[EmployeeId],
	[UserId],
	[Title],
	[FirstName],
	[LastName],
	[Email],
	[Street],
	[City],
	[State],
	[ZipCode],
	[Country],
	[Phone],
	[SSN],
	[IsActive],
	[IsDeleted],
	[IsInstaller],
	[IsSalesPerson],
	[IsServiceCall],
	[HireDate],
	[ProfilePicture],
	[Session],
	[JobTitle],
	[PlaceOfBirth],
	[SalesCommissionStructure],
	[TechCommissionStructure],
	[RecruitmentProcess],
	[Recruited],
	[CreatedDate],
	[Status],
	[LastUpdatedBy],
	[LastUpdatedDate]
				FROM 
					EmployeeEntries
				WHERE 
					Row between '+ CONVERT(nvarchar(10), (@PageIndex * @RowPerPage) + 1) +'And ('+ CONVERT(nvarchar(10), (@PageIndex * @RowPerPage) +@RowPerPage+ 1) +')'
	

SET @SQL2 =		' SELECT @TotalRows = COUNT(*) 
				FROM 
				[dbo].[Employee] ' + @WhereClause
								
EXEC sp_executesql @SQL2, N'@TotalRows int output', @TotalRows = @TotalRows output

EXEC sp_executesql @SQL1

RETURN @@ROWCOUNT
END
GO

