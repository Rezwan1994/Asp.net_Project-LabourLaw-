CREATE PROCEDURE InsertEmployee
(
	@Id int OUTPUT,
	@EmployeeId uniqueidentifier,
	@UserId uniqueidentifier,
	@Title nvarchar(50),
	@FirstName nvarchar(250),
	@LastName nvarchar(250),
	@Email nvarchar(50),
	@Street nvarchar(500),
	@City nvarchar(50),
	@State nvarchar(50),
	@ZipCode nvarchar(50),
	@Country nvarchar(50),
	@Phone nvarchar(50),
	@SSN nvarchar(50),
	@IsActive bit,
	@IsDeleted bit,
	@IsInstaller bit,
	@IsSalesPerson bit,
	@IsServiceCall bit,
	@HireDate datetime,
	@ProfilePicture nvarchar(250),
	@Session nvarchar(50),
	@JobTitle nvarchar(100),
	@PlaceOfBirth nvarchar(100),
	@SalesCommissionStructure nvarchar(100),
	@TechCommissionStructure nvarchar(100),
	@RecruitmentProcess bit,
	@Recruited bit,
	@CreatedDate datetime,
	@Status nvarchar(50),
	@LastUpdatedBy nvarchar(250),
	@LastUpdatedDate datetime
)
AS
    INSERT INTO [dbo].[Employee] 
	(
	[EmployeeId],
	[UserId],
	[Title],
	[FirstName],
	[LastName],
	[Email],
	[Street],
	[City],
	[State],
	[ZipCode],
	[Country],
	[Phone],
	[SSN],
	[IsActive],
	[IsDeleted],
	[IsInstaller],
	[IsSalesPerson],
	[IsServiceCall],
	[HireDate],
	[ProfilePicture],
	[Session],
	[JobTitle],
	[PlaceOfBirth],
	[SalesCommissionStructure],
	[TechCommissionStructure],
	[RecruitmentProcess],
	[Recruited],
	[CreatedDate],
	[Status],
	[LastUpdatedBy],
	[LastUpdatedDate]
    ) 
	VALUES 
	(
	@EmployeeId,
	@UserId,
	@Title,
	@FirstName,
	@LastName,
	@Email,
	@Street,
	@City,
	@State,
	@ZipCode,
	@Country,
	@Phone,
	@SSN,
	@IsActive,
	@IsDeleted,
	@IsInstaller,
	@IsSalesPerson,
	@IsServiceCall,
	@HireDate,
	@ProfilePicture,
	@Session,
	@JobTitle,
	@PlaceOfBirth,
	@SalesCommissionStructure,
	@TechCommissionStructure,
	@RecruitmentProcess,
	@Recruited,
	@CreatedDate,
	@Status,
	@LastUpdatedBy,
	@LastUpdatedDate
    )
	DECLARE @Err int
	DECLARE @Result int

	SET @Result = @@ROWCOUNT
	SET @Err = @@ERROR 
	If @Err <> 0 
	BEGIN
		SET @Id = -1
		RETURN @Err
	END
	ELSE
	BEGIN
		If @Result = 1 
		BEGIN
			-- Everything is OK
			SET @Id = @@IDENTITY
		END
		ELSE
		BEGIN
			SET @Id = -1
			RETURN 0
		END
	END

	RETURN @Id
GO

