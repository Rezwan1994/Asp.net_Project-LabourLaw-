* {
}

body {
    height: 100%;
}

.top {
    display: inline-block;
    overflow: hidden;
    width: 100%;
    padding-bottom: 15px;
}

header {
    background-color: #FAFAFA;
    height: 145px;
}


.txtsearch {
    float: right;
    width: 80%;
}

#chapter {
    display: block;
    width: 100%;
    color: black;
    font-weight: bold;
    padding: 5px 5px;
    padding-left: 16px;
    margin: 0px;
}
   
#chapter:hover {
    background-color: #DDDDDD;
}

.suggestion {
    width: calc(100% - 30px);
    display: block;
    background-color: white;
    border: 1px solid #DDDDDD;
    margin-top: -2px;
    cursor: pointer;
    display: none;
    position: absolute;
    z-index: 1;
}

.btnsearch {
    width: 70%;
    margin: auto;
    position: fixed;
    display: table;
    text-align: center;
    color: black;
}

#myInput {
    background-repeat: no-repeat;
    border: 1px solid #DDDDDD;
    padding: 10px;

    display: block;
}







.search_row {
    width: 1000px;
}

.search_content h3 {
    color: #1A0DAE;
    font-size: 16px;
}

.search_content p {
    color: #00591F;
    font-size: 12px;

    line-height: 1px;
}

#myTable1 tr.header, #myTable tr:hover {
    background-color: #f1f1f1;
}

.tab {
    overflow: hidden;
    padding: 0px 0px 0px 175px;
}

    /* Style the buttons inside the tab */
    .tab button {
        background-color: inherit;
        float: left;
        border: none;
        outline: none;
        cursor: pointer;
        transition: 0.3s;
        font-size: 17px;
    }

#search {
    background-image: url('Simpleicons_Interface_magnifying-glass.jpg');
}

/* Change background color of buttons on hover */
.tab button:hover {
    background-color: #ddd;
}

/* Create an active/current tablink class */
.tab button.active {
    background-color: #ccc;
}

/* Style the tab content */
.tabcontent {
    display: none;
    padding: 6px 12px;
}

#search_icon\ {
    font-size: 25px;
    width: 25px;
    margin-right: 80px;
}

