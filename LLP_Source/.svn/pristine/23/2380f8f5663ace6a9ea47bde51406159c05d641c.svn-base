CREATE PROCEDURE InsertInvoiceDetail
(
	@Id int OUTPUT,
	@InvoiceId nvarchar(15),
	@InventoryId uniqueidentifier,
	@EquipmentId uniqueidentifier,
	@EquipName nvarchar(500),
	@EquipDetail nvarchar(max),
	@CompanyId uniqueidentifier,
	@BundleId int,
	@Quantity int,
	@UnitPrice float,
	@TotalPrice float,
	@CreatedDate datetime,
	@CreatedBy nvarchar(50)
)
AS
    INSERT INTO [dbo].[InvoiceDetail] 
	(
	[InvoiceId],
	[InventoryId],
	[EquipmentId],
	[EquipName],
	[EquipDetail],
	[CompanyId],
	[BundleId],
	[Quantity],
	[UnitPrice],
	[TotalPrice],
	[CreatedDate],
	[CreatedBy]
    ) 
	VALUES 
	(
	@InvoiceId,
	@InventoryId,
	@EquipmentId,
	@EquipName,
	@EquipDetail,
	@CompanyId,
	@BundleId,
	@Quantity,
	@UnitPrice,
	@TotalPrice,
	@CreatedDate,
	@CreatedBy
    )
	DECLARE @Err int
	DECLARE @Result int

	SET @Result = @@ROWCOUNT
	SET @Err = @@ERROR 
	If @Err <> 0 
	BEGIN
		SET @Id = -1
		RETURN @Err
	END
	ELSE
	BEGIN
		If @Result = 1 
		BEGIN
			-- Everything is OK
			SET @Id = @@IDENTITY
		END
		ELSE
		BEGIN
			SET @Id = -1
			RETURN 0
		END
	END

	RETURN @Id
GO

